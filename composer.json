{
    "name": "veseluy-rodjer/laravel-repository-generator",
    "description": "Generate Laravel repositories",
    "keywords": ["generator", "php", "cli", "generator", "laravel", "artisan"],
    "homepage": "https://github.com/veseluy-rodjer/laravel-repository-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Veseluy Rodjer"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/console": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "VeseluyRodjer\\RepositoryGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VeseluyRodjer\\RepositoryGenerator\\RepositoryGeneratorServiceProvider"
            ]
        }
    }
}
